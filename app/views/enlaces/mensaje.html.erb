            <div class="mt-3 flex align-middle">
                <div class="flex w-1/3">
                    <div><%= inline_svg_tag 'svg/adoc.svg' %></div>
                            <div class="ml-1">Mensajes enviados</div>
                    </div>
                 </div>
            </div> 
            <div class="p-5 border border-gray-200 rounded-lg shadow sm:p-3 dark:bg-gray-800 dark:border-gray-700">
                <ol class="relative text-gray-500 border-l border-gray-200 dark:border-gray-700 dark:text-gray-400">                  
                        <% @proyecto.enlaces.where(enevento_id:1).order(created_at: :desc).each do |m| %>
