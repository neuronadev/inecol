 <div class="font-semibold border-b dark:bg-gray-800 dark:border-gray-700 p-2 text-gray-800 font-semibold"> Monto Ejercido</div>
<div class="mt-2">
     <div class="flex">
           <%= link_to new_proyecto_mtoejercido_path(@proyecto), class:"flex text-blue-800" do %>
                    <div><%= inline_svg_tag 'svg/mas-blue.svg' %></div> <div class="ml-1 text-sm font-semibold">Nuevo</div>
           <% end %>
     </div>
     <div class="mt-3">
        <% if @proyecto.mtoejercidos.any? %>
            <%= render 'listado' %>
        <% else %>
            No hay registros.
        <% end %>
    </div>    
</div>