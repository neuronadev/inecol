<table style="width:100%; border-collapse: collapse;">
                                   <% i = 1 %>
                                   <% @proyecto.participante.pacademicos.each do |acad| %>
                                        <% if i < @proyecto.participante.pacademicos.count(:all) %>
                                            <tr style="background-color: #FFFFFF; border-bottom: 1px solid #a9a9a9;">
                                               <td style=""> <%= acad.academico.persona.nom_espacio  %> </td>
                                               <td style="width:10%; text-align: center; background-color: #FFFFFF; border-left: 1px solid #a9a9a9;"> <%=  sprintf("%.0f",acad.porcentaje)  %></td>
                                            </tr>
                                        <% else %>    
                                            <tr style="background-color: #FFFFFF;">
                                               <td style=""> <%= acad.academico.persona.nom_espacio  %> </td>
                                               <td style="width:10%; text-align: center; background-color: #FFFFFF; border-left: 1px solid #a9a9a9;"> <%= sprintf("%.0f",acad.porcentaje) %></td>
                                            </tr>
                                        <% end %>        
                                   <% i += 1 %>       
                                   <% end %>    
</table>