<tr class="itempy-<%= py.id %> bg-white border-t dark:bg-gray-900 dark:border-gray-700 box" 
    id="<%= py.id %>">
        <td style="text-align: center;">
           <svg data-action="click->organizar#selFila"
                data-organizar-iditems-param="itempy-<%= py.id %>"
                style="cursor: pointer;"
                class="w-6 h-5 text-blue-400 dark:text-white" 
                aria-hidden="true" xmlns="http://www.w3.org/2000/svg" 
                width="24" 
                height="24" 
                fill="currentColor" 
                viewBox="0 0 24 24">
                    <path fill-rule="evenodd" d="M11.403 5H5a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h12a2 2 0 0 0 2-2v-6.403a3.01 3.01 0 0 1-1.743-1.612l-3.025 3.025A3 3 0 1 1 9.99 9.768l3.025-3.025A3.01 3.01 0 0 1 11.403 5Z" clip-rule="evenodd"/>
                    <path fill-rule="evenodd" d="M13.232 4a1 1 0 0 1 1-1H20a1 1 0 0 1 1 1v5.768a1 1 0 1 1-2 0V6.414l-6.182 6.182a1 1 0 0 1-1.414-1.414L17.586 5h-3.354a1 1 0 0 1-1-1Z" clip-rule="evenodd"/>
           </svg>
        </td>  
        <td style="text-align: center;">
            <% if py.enlaces.last.enevento.clave == 'FIR' || py.enlaces.last.enevento.clave == 'DICT' %>
                    <%= py.dictamen.numregistro %>
            <% else %>       
                --
            <% end %>
            
    </td>
    <th scope="row" class="p-2 font-medium text-gray-900  dark:text-white" style="text-align: justify;">
            <%= py.nombre %>
            
    </th>
    <td class="p-2" style="text-align: center;">
        <%= py.etapas.includes(:tevento).where('teventos.clave':'REV').first.created_at.strftime("%d-%m-%Y %H:%M ")  %>
    </td>
    <td class="p-2" style="text-align: center;">
        <%= @resp %>
    </td>
    
    <td class="p-2" style="text-align: center;">
        <%= render 'vistas/util/elestado', proyecto:py %>
    </td>
    <td class="p-2" style="text-align: center;">
        <% if py.etapas.last.tevento.clave == 'CAP' %> 
            <%= link_to 'Editar', resumen_vistas_path(:id=>py.id), class:"font-medium text-blue-600 dark:text-blue-500 hover:underline" %> 
        <% else %>    
            <%= link_to 'Ver', resumen_vistas_path(:id=>py.id), class:"font-medium text-blue-600 dark:text-blue-500 hover:underline",  data:{turbo_frame: 'pycontent'}  %> 
        <% end %>                            
    </td>
</tr>
<%= render "proyectos/rw_enlace_mod", py: py %> 