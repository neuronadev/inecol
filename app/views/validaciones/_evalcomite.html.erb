                    <th scope="row" class="px-6 py-4 font-medium text-gray-900 whitespace-nowrap dark:text-white">
                         <%= e.persona.nom_espacio %>
                    </th>
                    <td class="px-6 py-4">
                         <%= render 'estadoval', proyecto:@proyecto, evaluador:e %> 
                    </td>
                    <td class="px-6 py-4">
                        ---
                    </td>
                </tr>
<% end %>    