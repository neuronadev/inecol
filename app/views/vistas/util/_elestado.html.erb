 <div class="text-blue-700 flex">
     <%  if proyecto.enlaces.any? %>
                <span class="flex items-center text-sm font-medium text-gray-900 dark:text-white">
                        <% if proyecto.enlaces.order(:created_at).last.enevento.clave == 'DICT' %>
                                <span class="flex w-2.5 h-2.5 bg-green-600 rounded-full mr-1.5 flex-shrink-0"></span>
                        <% elsif proyecto.enlaces.order(:created_at).last.enevento.clave == 'FIR' %>
                                <span class="flex w-2.5 h-2.5 bg-blue-800 rounded-full mr-1.5 flex-shrink-0"></span>
                         <% elsif proyecto.enlaces.order(:created_at).last.enevento.clave == 'EVAL' %>        
                                <span class="flex w-2.5 h-2.5 bg-pink-800 rounded-full mr-1.5 flex-shrink-0"></span>
                        <% else %>
                                <span class="flex w-2.5 h-2.5 bg-orange-600 rounded-full mr-1.5 flex-shrink-0"></span>
                        <% end %>   
                        
                        <% if proyecto.modificatorio == 'SI' %>
                               <% if proyecto.enlaces.order(:created_at).last.enevento.clave == 'DICT' %>
                                           Evaluado
                               <% else %>
                                           <%= proyecto.enlaces.order(:created_at).last.enevento.nomenevento %> 
                               <% end %>
                        <% else %>        
                                 <%= proyecto.enlaces.order(:created_at).last.enevento.nomenevento %>
                        <% end %>
                         
                            

                        <% if proyecto.enlaces.order(:created_at).last.enevento.clave == 'DICT' %>
                               <%= proyecto.dictamen.created_at.strftime("%d-%m-%Y %H:%M ")  %>
                        <% end %>

                </span>
     <% else %>
                 <% if proyecto.modificatorio == 'SI' && current_usuario.cuenta.rol.clave == 'EL' %>
                         <span class="flex items-center text-sm font-medium text-gray-900 dark:text-white">
                         <span class="flex w-2.5 h-2.5 bg-red-500 rounded-full mr-1.5 flex-shrink-0"></span>Modificaci√≥n</span>  
                 <% else %>
                         <span class="flex items-center text-sm font-medium text-gray-900 dark:text-white">
                         <span class="flex w-2.5 h-2.5 bg-teal-500 rounded-full mr-1.5 flex-shrink-0"></span>Nuevo</span>
                <% end %>         
     <% end %>
 </div>