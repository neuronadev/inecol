                     <div class="flex hover:cursor-pointer font-medium text-sm leading-tight">
                        <div> <%= inline_svg_tag 'svg/girar-iz' %></div> <div class="ml-2 text-sky-800">Solicitar cambios</div>
                     </div>   
               <% end %>      
         </div>     
        
        <div>
