<div class="w-full flex justify-end">
        
      <div class="flex mt-5 mb-1 w-3/4 justify-between">
            <div >
                 <%= link_to proyectos_path, 'data-frame-target':'_top', class:'flex' do %>
                        <%= inline_svg_tag 'svg/hogar.svg' %>
                        <span class='text-sm text-blue-800 font-semibold ml-1'>Inicio</span>
                  <% end %>      
            </div>  
            <div>
                  <%= link_to  resumen_vistas_path(:id=>@proyecto.id), data:{turbo_frame: "py_content_sub"} do %>
                          <div class="flex hover:cursor-pointer font-medium text-sm leading-tight">
                                      <div> <%= inline_svg_tag 'svg/proyecto1.svg' %></div> <div class="ml-2 text-blue-800">Proyecto</div>
                          </div>   
                  <% end %>      
            </div>  
            <% if @proyecto.enlaces.any? %>
                  <% if @proyecto.enlaces.last.enevento.clave == 'NVO' || @proyecto.enlaces.last.enevento.clave == 'PROC' %>
                              <%= render 'vistas/util/m_nvo_proc' %>
                  <% end %>
                  <% if @proyecto.enlaces.last.enevento.clave == 'CORR' %>
                              <%= render 'vistas/util/m_corr' %>
                  <% end %>
                  <% if @proyecto.enlaces.last.enevento.clave == 'EVAL' %>
                              <%= render 'vistas/util/m_eval' %>
                  <% end %>
                  <% if @proyecto.enlaces.last.enevento.clave == 'DICT' %>
                               <%= render 'vistas/util/m_dict' %>
                  <% end %>
                  
            <% end %>        
      </div>
</div>        